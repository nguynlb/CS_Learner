<Application x:Class="NavigationDI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NavigationDI"
             xmlns:viewModel="clr-namespace:NavigationDI.MVVM.ViewModels"
             xmlns:view="clr-namespace:NavigationDI.MVVM.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <DataTemplate DataType="{x:Type viewModel:HomeViewModel}" >
            <view:HomeView />
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewModel:SettingViewModel}" >
            <view:HomeView />
        </DataTemplate>


    </Application.Resources>
</Application>
